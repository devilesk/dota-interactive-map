{
  "name": "dota-interactive-map",
  "version": "5.2.0",
  "description": "Interactive Dota 2 Map",
  "main": "src/js/index.js",
  "private": true,
  "scripts": {
    "render": "node scripts/render.js",
    "markdown:help": "marked src/template/help.md -o src/template/help.html",
    "preprocess:html": "npm run markdown:help && node scripts/preprocess.js",
    "generatejson": "rollup -c rollup.config.scripts.js && node scripts/generateGeoJson.js",
    "build": "mkdir -p build && npm run build:js && npm run build:css && npm run build:html && npm run build:copy",
    "build:js": "rollup -c",
    "build:css": "node scripts/css.js",
    "build:html": "npm run render && npm run preprocess:html",
    "build:copy": "cp -r assets/* build && mkdir -p build/save",
    "build:clean": "rm -rf build",
    "watch": "npm-run-all --parallel watch:*",
    "watch:js": "rollup -c -w",
    "watch:css": "nodemon -e scss --watch src/scss -x \"npm run build:css\"",
    "watch:html": "nodemon -e j2,md --watch src -x \"npm run build:html\"",
    "dist": "npm run build:clean && NODE_ENV=production npm run build",
    "rollbar:sourcemap": "gulp rollbar:sourcemap",
    "rollbar:deploy": "node scripts/rollbar_deploy.js",
    "rollbar": "npm run rollbar:sourcemap && npm run rollbar:deploy -- production",
    "deploy": "node scripts/deploy.js && npm run rollbar",
    "gh-pages": "git subtree push --prefix build origin gh-pages",
    "serve": "npx http-server build -p 3000"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/devilesk/dota-interactive-map.git"
  },
  "author": "devilesk",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/devilesk/dota-interactive-map/issues"
  },
  "homepage": "https://github.com/devilesk/dota-interactive-map#readme",
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@turf/union": "^5.1.5",
    "clean-css": "^4.1.9",
    "del": "^4.1.1",
    "dotenv": "^7.0.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.17.2",
    "file-saver": "^1.3.8",
    "git-rev-sync": "^1.8.0",
    "gulp": "^3.9.1",
    "gulp-rollbar": "^0.1.4",
    "gulp-sourcemaps": "^2.6.2",
    "jinjs": "^0.3.6",
    "js-yaml": "^3.10.0",
    "marked": "^0.6.2",
    "node-sass": "^4.6.0",
    "nodemon": "^1.12.1",
    "npm-run-all": "^4.1.3",
    "preprocess": "^3.1.0",
    "rollbar": "^2.4.4",
    "rollup": "^1.10.1",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^4.2.3",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-terser": "^4.0.4"
  },
  "dependencies": {
    "dota-vision-simulation": "^0.17.0",
    "ol": "^5.2.0",
    "ol-rotate-feature": "^2.0.0"
  }
}
